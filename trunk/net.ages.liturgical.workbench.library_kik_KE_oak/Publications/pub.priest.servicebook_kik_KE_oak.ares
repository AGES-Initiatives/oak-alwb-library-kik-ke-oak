li.prokimenonmode = "Wĩrutĩri (prokimeno)."
li.blessreader = "Mũnene, rathima mũthomi wa Ũhoro Mwega wa (Mathayo, Mariko, Luka, Johana), mũtũmwo mũtheru na mwandĩki wa Evangelio (Injiri)."
li.blessreader2 = "Kũhĩtũkĩra mahoya-inĩ ma mũtũmwo (rĩĩtwa), mũtheru na mũkumie na mwandĩki wa Evangelio, Ngai akũhe kiugo hamwe na hinya mũnene, nĩ getha Ũhoro mwega wa Mwathani witũ Jesu Kristo Mũriũ wa Ngai, ũrĩa mwende, ũhingio."
li.rubric.readgospel = "Ndikoni athomaga ũhoro wa mũthenya. Arĩkia gũthoma,"
li.greatlitany.title = "MAHOYA MA GŨTHAITHANA (EKTENIA)"
li.rubric.thriceeach = "(x 3)"
li.pet19lash = "Makĩria nĩtwakũhoya nĩ ũndũ wa Mũmbicobu witũ mũnene (Archbishop) (rĩĩtwa)."
li.pet21lash = "Makĩria nĩtwakũhoya nĩ ũndũ wa tha, muoyo, thayũ, ũgima, ũhonokio, ũgitĩri, woheri na ũthengia wa mawĩhia ma ndungata cia Ngai, akristiano ahooreri oothe a Orthodox, arĩa matũraga itũũra rĩrĩ na ahĩtũkĩri, akristiano a kanitha ĩno, na kĩama kĩayo na arĩa matungatagĩra nyũmba ĩno theru."
li.pet22lash = "Makĩria nĩtwakũhoya nĩ ũndũ wa arathime, na aririkane arĩa maambĩrĩirie kanitha ĩno theru na oothe arĩa makomete, maithe maitũ a Orthodox, etĩkia arĩa matigĩte muoyo ũyũ mbere iitũ, arĩa me thĩ-inĩ wa Kristo, arĩa me haha na thĩ-inĩ wa thĩ yoothe."
li.rubric.catechumens = ""
li.prayercatechumens.title = "MAHOYA NĨ ŨNDŨ WA AKATEKUMENO (ARUTWO A KĨRĨRA)"
li.liturgyfaithful.title = "IGONGONA RĨA ETĨKIA"
li.prayer1faithful.title = "Mahoya ma mbere ma etĩkia"
li.prayer2faithful.title = "Mahoya ma kerĩ ma etĩkia"
li.greatentrance.title = "RĨINGĨRĨRO INENE RĨA MŨROMO MŨTHERU"
li.rubric.cherubic = "Rũĩmbo rũa Akerubi"
li.prayercherubic.title = "Mahoya ma rũĩmbo rũa Akerubi"
li.cherubic2 = "Nĩ getha twamũkĩre mũthamaki wa oothe, ũrĩa umagaragio nĩ mbũtũ cia araika matekuoneka. Alleluia. Alleluia. Alleluia."
li.rubric.censeatcherubikon = "Thutha wa ihoya rĩrĩ mũrathimithia nĩahũraga ũbani (incense) metha-inĩ theru, na ikonostasi, na kũrĩ andũ, akigaga kahora rũĩmbo “Nĩtũrĩkĩtie kuona kũriũka gwa Kristo” na thaburi ya 51. Thutha wa kũhoya mbere ya metha theru na kũhũgũka kũrĩ andũ, na makoiga me hamwe na ndikoni:"
li.forgive = "Arĩ na ariũ akwa njoherai niĩ mũndũ mwĩhia."
li.eparon = "Oya na igũrũ, Mũnene."
li.rubric.psa133.v2.fullattribution = "(Thaburi ya 133:2)"
li.rubric.deaconright = "Marĩkia gũtonya thĩ-inĩ wa Metha Theru, ndikoni akoiga:"
li.holydiaconate = "Mwathani Ngai witũ aririkane ũtungati waku thĩ-inĩ wa ũthamaki wake, hĩndĩ ciothe, rĩu na tene, na kinya tene na tene."
li.rubric.placegiftsaltar = "Aigĩrĩra iheo theru igũrũ rĩa Metha Theru nĩoigaga kahora:"
li.noblejoseph = "Jusufu wa gĩtĩyo, rĩrĩa aharũrũkirie mwĩrĩ waku ũrĩa ũtarĩ karema kuuma mũtharaba-inĩ, nĩaũthaire na taama mwega hamwe na maguta manungaga wega, na akĩũiga thĩ-inĩ wa mbĩrĩra njeru na akĩmĩhinga."
li.rubric.psa50.v20.v21.fullattribution = "(Thaburi ya 50:21)"
li.rubric.rememberpriest = "Akera ndikoni:"
li.litanygifts.title = "GUTHAITHANA KWA IHEO THERU"
li.rubric.kissofpeace = "Angĩkorũo kwĩna arathimithia makĩria ya ũmwe, oothe no makamumunya iheo theru o ũmwe thutha wa ũrĩa ũngĩ na makamumunyana o ene. Na mũrathamithia ũrĩa ũtongoretie igongona akoiga atĩrĩ:"
li.christmidst1 = "Kristo e gatagatĩ-inĩ gaitũ."
li.rubric.kissofpeace2 = "Nake ũcio ũngĩ agacokia akoiga:"
li.christmidst2 = "Nĩ ma e gatagatĩ-inĩ gaitũ, na egũtũũra ho tene na tene."
li.creed.title = "WĨTĨKIO WA NIKEA"
li.tasa = "Nĩtwakũhee indo ciaku, iria ciumĩte indo-inĩ ciaku, nĩ ũndũ wa woothe na nĩ ũndũ wa moothe."
li.seimnumen = "Wee nĩtũrakũinĩra, nĩtũrakũrathima, nĩtũracokia ngaatho kũrĩ wee, ũi Mwathani, na nĩtũrahoya kũrĩ wee, ũi Ngai witũ."
li.changebread = "Mũnene, rathima mũgate mũtheru."
li.changewine = "Mũnene, rathima gĩkombe gĩtheru."
