pet21p = ""

pet22 = ""

pet23 = "Makĩria nĩtwakũhoya nĩ ũndũ wa arĩa marehage matunda na makaruta wĩra mwega, thĩ-inĩ wa kanitha ĩno theru na ya gĩtĩyo, na nĩ ũndũ wa arĩa marutaga wĩra wa ũtungati, na aini, na andũ arĩa me hakuhĩ arĩa metereire momĩrĩirie mena mwĩhoko waku mũnene wa tha nyingĩ."

pet24 = "Rekei tũrĩkie mahoya maitũ kũrĩ Mwathani." // Complementary

pet24v = ""

pet24m = ""

pet25v = ""

pet25m = "Nĩ ũndũ wa mũthenya ũyũ woothe ũtuĩke mwega, mũtheru, wĩ thayũ, na ũtarĩ na wĩhia, rekei tũũrie Mwathani."

pet26 = "Nĩ ũndũ wa mũraika wa thayũ, mũtongoria mwĩtĩkia, na mũrangĩri wa mĩoyo iitũ, o na mĩĩrĩ iitũ, rekei tũũrie Mwathani."

pet27 = "Nĩ ũndũ wa woheri na gũthengerio kwa mawĩhia na maũũru maitũ, rekei tũũrie Mwathani."

pet28 = "Nĩ ũndũ wa indo ciothe iria njega, na ii na ũmithio harĩ mĩoyo-inĩ iitũ, na nĩ ũndũ wa thayũ wa thĩ yoothe, rekei tũũrie Mwathani."

pet29 = "Na nĩguo tũhote kũrĩkia ihinda rĩrĩa rĩtigaru rĩa mĩoyo iitũ twĩna thayũ na wĩriro, rekei tũũrie Mwathani."

pet30 = "Na nĩguo mĩtũũrĩre iitũ ya kũrigĩrĩa ĩkorũo ĩ ya ũkristiano, ĩtarĩ na ruo, ũcuuke na ĩĩna thayũ; na kwĩrigĩrĩria kwega mbere ya ũgwati wa cira ũrĩa ũgaikarĩrũo gĩtĩ nĩ Kristo, rekei tũũrie Mwathani."

pet31 = "Thayũ ũrogĩa na inyuĩ."

pet32 = "Rekei tũinamie mĩtwe iitũ mbere ya Mwathani."

