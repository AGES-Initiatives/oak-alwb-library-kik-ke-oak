hoMA.DoxologyGreat.title = ""
hoMA.DoxologyGreatIntro.text = "Ũrokumio wee wonanire ũtheri."
hoMA.DoxologyVerse01.text = le.go.os_kik_KE_oak.leosMA.GreatDoxology.v01.text
hoMA.DoxologyVerse02.text = "Nĩtũrakũinĩra wee, nĩtũrakũrathima wee, nĩtũrakũinamĩrĩra wee, nĩtũragũkumia wee, nĩtũragũcokeria ngaatho, tondũ wa ũkumio waku mũnene."
hoMA.DoxologyVerse03.text = "Wee Mwathani Mũthamaki, Ngai wa igũrũ, Ithe mwene hinya woothe; wee Mwathani Mũriũ wake wa mũmwe, Jesu Kristo, na wee Muoyo Mũtheru."
hoMA.DoxologyVerse04.text = "Wee Mwathani Ngai, gatũrũme ka Ngai, Mũriũ wa Ithe, ũrĩa ũthengagia wĩhia wa thĩ, ũtũiguĩre tha, wee ũthengagia mehia ma thĩ."
hoMA.DoxologyVerse05.text = "Wee ũikaraga guoko-inĩ kwa ũrĩo gwa Ithe, ĩtĩkĩra mahoya maitũ, na ũtũiguĩre tha."
hoMA.DoxologyVerse06.text = "Tondũ wee wiki nowe Mũtheru, wee wiki nowe Mwathani, Jesu Kristo, nĩ ũndũ wa ũkumio wa Ngai Ithe. Amen."
hoMA.DoxologyVerse07.text = ps_kik_KE_oak.psa144.v2.text
hoMA.DoxologyVerse07e.text = ""
hoMA.DoxologyVerse08.text = "Tũtue agĩrĩru, ũi Mwathani, na ũtũige mũthenya ũyũ tũtarĩ na mehia."
hoMA.DoxologyVerse08e.text = ""
